{
    "authentication": {
        "type": "jwt"
    },
    "baseUrl": "<%= base_url %>",
    "contexts": [
        "personal"
    ],
    "description": "Discover and keep track of your favorite repos with Stars for BitBucket!",
    "key": "<%= plugin_key %>",
    "name": "<%= plugin_name %>",
    "scopes": [
        "read",
        "write"
    ],
    "lifecycle": {
        "installed": "/installed",
        "uninstalled": "/uninstalled"
