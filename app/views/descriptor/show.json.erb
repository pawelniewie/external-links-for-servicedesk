{
  "authentication": {
    "type": "jwt"
  },
  "baseUrl": "<%= base_url %>",
  "contexts": [
    "personal"
  ],
  "description": "Add custom links to external services on Service Desk tickets",
  "key": "<%= plugin_key %>",
  "name": "<%= plugin_name %>",
  "scopes": [
    "read",
    "write"
  ],
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "vendor": {
    "name": "Pawel Niewiadomski",
    "url": "https://pawelniewiadomski.com"
  },
  "modules": {
    "generalPages": [
      {
        "key": "custom-feedback-project-config",
        "location": "atl.jira.proj.config/projectgroup4",
        "name": {
          "value": "Service Links"
        },
        "url": "/projects/{project.id}/service_links"
      }
    ],
    "jiraEntityProperties": [
      {
        "name": {
          "value": "Customer Feedback"
        },
        "entityType": "issue",
        "key": "customer-feedback-entity-property",
        "keyConfigurations": [
          {
            "propertyKey": "customer-feedback",
            "extractions": [
              {
                "objectName": "rating",
                "type": "number",
                "alias": "customerFeedbackRating"
              },
              {
                "objectName": "comment",
                "type": "text",
                "alias": "customerFeedbackComment"
              }
            ]
          }
        ]
      }
    ]
  }
}