{
    "authentication": {
        "type": "jwt"
    },
    "baseUrl": "<%= base_url %>",
    "contexts": [
        "personal"
    ],
    "description": "Discover and keep track of your favorite repos with Stars for BitBucket!",
    "key": "<%= plugin_key %>",
    "name": "<%= plugin_name %>",
    "scopes": [
        "read",
        "write"
    ],
    "lifecycle": {
        "installed": "/installed",
        "uninstalled": "/uninstalled"
    },
    "vendor": {
        "name": "Pawel Niewiadomski",
        "url": "https://pawelniewiadomski.com"
    },
    "modules": {
      "configurePage": {
        "url": "<%= base_url %>/configure",
        "name": {
          "value": "Configuration"
        },
        "key": "config-page"
      },
      "generalPages": [
        {
          "key": "custom-feedback-project-config",
          "location": "atl.jira.proj.config/projectgroup4",
          "name": {
            "value": "Customer feedback"
          },
          "url": "<%= base_url %>/projects/${project.id}/configure"
        }
      ]
    }
}