{
    "authentication": {
        "type": "jwt"
    },
    "baseUrl": "<%= base_url %>",
    "contexts": [
        "personal"
    ],
    "description": "Blah blah blah",
    "key": "<%= plugin_key %>",
    "name": "Stars for BitBucket",
    "scopes": [
        "account",
        "repository"
    ],
    "lifecycle": {
        "installed": "/installed",
        "uninstalled": "/uninstalled"
    },
    "modules": {
      "oauthConsumer": {
        "clientId": "<%= client_id %>"
      },
      "webPanel": [
        {
          "url": "/stars?repoPath={repo_path}",
          "name": {
            "value": "Stars Web Panel"
          },
          "location": "org.bitbucket.repository.overview.informationPanel",
          "key": "stars-web-panel"
        }
      ],
      "profileTab": [
        {
          "url": "/all-stars",
          "name": {
            "value": "Your stars"
          },
          "key": "all-stars"
        }
      ]
    },
    "vendor": {
        "name": "Pawel Niewiadomski",
        "url": "https://pawelniewiadomski.com"
    }
}